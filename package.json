{
  "name": "mnml",
  "description": "A minimal, responsive, fast ghost blog theme with great typography. Comes with Disqus comments, syntax highlighting, and KaTeX for mathematics, and more.",
  "demo": "https://mnml.curiositry.com/",
  "version": "7.1.2",
  "engines": {
    "ghost": ">=5.104.0"
  },
  "author": {
    "name": "Curiositry",
    "email": "info@automathic.org",
    "url": "http://curiositry.com"
  },
  "config": {
    "posts_per_page": 5,
    "card_assets": {
      "exclude": [
        "bookmark",
        "gallery"
      ]
    }
  },
  "gpm": {
    "type": "theme",
    "categories": [
      "Minimal",
      "Responsive",
      "Personal Blogs",
      "Typographic"
    ]
  },
  "keywords": [
    "responsive",
    "ghost",
    "minimal",
    "theme",
    "ghost-theme"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/curiositry/mnml-ghost-theme.git"
  }
}
